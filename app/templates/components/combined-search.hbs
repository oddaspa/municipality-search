<div class="input-section">
  {{input
    value=this.value
    key-up=(action "handleFilterEntry")
    class="light"
    placeholder="Filter By Name"
  }}



  <label class="checkbox-container">
    {{input
      type="checkbox"
      checked=cbState
      click=(action "changeState")}}
      <span class="checkmark"></span>
      <span class="valid-check"> Active only</span>
  </label>
</div>


{{yield this.results}}
